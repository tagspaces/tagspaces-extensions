{
  "name": "@tagspaces/media-player",
  "version": "1.0.162",
  "description": "A TagSpaces extension allowing playing of audio and video files.",
  "homepage": "./",
  "files": [
    "build/"
  ],
  "author": {
    "name": "TagSpaces GmbH",
    "email": "info@tagspaces.org",
    "url": "https://www.tagspaces.org"
  },
  "license": "MIT",
  "scripts": {
    "dev": "vite dev --host",
    "build": "tsc && vite build",
    "build-dev": "tsc && vite build --mode development",
    "preview": "vite preview",
    "code-format": "npx prettier --write .",
    "npmpack": "npm run build-dev && npx shx cp -rL build/** ../../release/app/node_modules/@tagspaces/extensions/md-editor-crepe/build && npx shx cp package.json ../../release/app/node_modules/@tagspaces/extensions/md-editor-crepe"
  },
  "dependencies": {
    "@mui/icons-material": "^6.4.4",
    "@mui/material": "^6.4.4",
    "@tagspaces/tagspaces-codemirror": "^4.3.6",
    "@tagspaces/tagspaces-extension-ui": "^4.5.46",
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.0",
    "@tagspaces/tagspaces-common": "^4.5.51",
    "@milkdown/crepe": "^7.6.2",
    "@milkdown/kit": "^7.6.2",
    "@milkdown/react": "^7.6.2",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.2.0",
    "typescript": "^5.3.0",
    "vite": "^6.1.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "tsextension": {
    "name": "MD Editor (crepe)",
    "types": [
      "viewer",
      "editor"
    ],
    "color": "#beefed",
    "fileTypes": [
      {
        "ext": "md"
      },
      {
        "ext": "mdx",
        "types": [
          "viewer"
        ]
      }
    ],
    "buildFolder": "build"
  }
}