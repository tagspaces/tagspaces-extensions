{
  "name": "@tagspaces/md-editor",
  "version": "1.0.0",
  "description": "TagSpaces extension for editing notes in markdown format",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "parcel src/index.html",
    "build": "parcel build --no-minify --no-source-maps --no-autoinstall --no-cache --out-dir build --no-content-hash --public-url ./ src/index.html"
  },
  "keywords": [
    "md",
    "markdown",
    "editor",
    "tagspaces"
  ],
  "author": {
    "name": "TagSpaces UG",
    "email": "info@tagspaces.org",
    "url": "https://www.tagspaces.org"
  },
  "license": "MIT",
  "dependencies": {
    "@milkdown/core": "*",
    "@milkdown/preset-commonmark": "^4.9.4",
    "@milkdown/react": "^4.9.3",
    "@milkdown/theme-nord": "^4.9.4"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "parcel-bundler": "^1.12.5",
    "typescript": "^4.3.5"
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ]
  }
}
