{
  "name": "@tagspaces/md-editor",
  "version": "1.0.0",
  "description": "TagSpaces extension for editing notes in markdown format",
  "homepage": "./",
  "main": "build/index.js",
  "files": [
    "build/"
  ],
  "scripts": {
    "postinstall": "SKIP_PREFLIGHT_CHECK=true react-scripts build",
    "start": "react-scripts start",
    "build": "SKIP_PREFLIGHT_CHECK=true react-scripts build",
    "build:dev": "react-app-rewired build",
    "build:env": "npx env-cmd -f .env.development react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "code-format": "npx prettier --write .",
    "pkg-publish": "npm publish --access public"
  },
  "keywords": [
    "md",
    "markdown",
    "editor",
    "tagspaces"
  ],
  "author": "TagSpaces UG <info@tagspaces.org> (https://www.tagspaces.org)",
  "license": "MIT",
  "dependencies": {
    "@codemirror/basic-setup": "^0.19.0",
    "@codemirror/lang-markdown": "^0.19.1",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@milkdown/core": "^4.13.1",
    "@milkdown/plugin-clipboard": "^4.13.1",
    "@milkdown/plugin-diagram": "^4.13.1",
    "@milkdown/plugin-emoji": "^4.13.1",
    "@milkdown/plugin-history": "^4.13.1",
    "@milkdown/plugin-listener": "^4.13.1",
    "@milkdown/plugin-math": "^4.13.1",
    "@milkdown/plugin-prism": "^4.13.1",
    "@milkdown/plugin-slash": "^4.13.1",
    "@milkdown/plugin-table": "^4.13.1",
    "@milkdown/plugin-tooltip": "^4.13.1",
    "@milkdown/preset-commonmark": "^4.13.1",
    "@milkdown/preset-gfm": "^4.13.1",
    "@milkdown/react": "^4.13.1",
    "@milkdown/theme-nord": "^4.13.1",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "material-design-icons": "^3.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-loading-skeleton": "^2.2.0",
    "remark-directive": "^2.0.0"
  },
  "devDependencies": {
    "react-app-rewired": "^2.1.8",
    "react-scripts": "4.0.3",
    "typescript": "4.4.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tagspaces/tagspaces-extensions.git"
  },
  "bugs": {
    "url": "https://github.com/tagspaces/tagspaces-extensions/issues"
  }
}
