{
  "name": "@tagspaces/md-editor",
  "version": "2.0.0",
  "description": "TagSpaces extension for editing notes in markdown format",
  "tsextension": {
    "id": "@tagspaces/extensions/@tagspacesmd-editor_2.0.0",
    "name": "MD Editor",
    "types": [
      "viewer",
      "editor"
    ],
    "color": "#beefed",
    "fileTypes": [
      {
        "ext": "md"
      },
      {
        "ext": "mdx",
        "types": [
          "viewer"
        ]
      }
    ],
    "buildFolder": "build"
  },
  "homepage": "./",
  "main": "build/index.js",
  "files": [
    "build/"
  ],
  "scripts": {
    "start": "react-scripts start",
    "build": "npx cross-env GENERATE_SOURCEMAP=false react-scripts build",
    "build:dev": "react-app-rewired build",
    "build:env": "npx env-cmd -f .env.development react-scripts build",
    "zip": "npm run build && npx npm-build-zip --source=.",
    "zip:dev": "npm run build:dev && npx npm-build-zip --source=.",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "code-format": "npx prettier --write .",
    "npmpack": "npx shx cp -rL build/** ../../app/node_modules/@tagspaces/extensions/md-editor/build && npx shx cp package.json ../../app/node_modules/@tagspaces/extensions/md-editor",
    "build:pack": "npm run build && npm run npmpack",
    "pkg-publish": "npm publish --access public"
  },
  "keywords": [
    "md",
    "markdown",
    "editor",
    "tagspaces"
  ],
  "author": "TagSpaces UG <info@tagspaces.org> (https://www.tagspaces.org)",
  "license": "MIT",
  "dependencies": {
    "@tagspaces/tagspaces-md": "3.0.13",
    "@tagspaces/tagspaces-codemirror": "3.0.1",
    "@tagspaces/tagspaces-extension-ui": "^3.0.3",
    "i18next": "^22.4.6",
    "i18next-http-backend": "^2.2.1",
    "i18next-browser-languagedetector": "^7.0.2",
    "mark.js": "^8.11.1",
    "markmap-common": "^0.14.2",
    "markmap-lib": "^0.14.3",
    "markmap-view": "^0.14.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.1.1",
    "easy-speech": "^1.2.1"
  },
  "devDependencies": {
    "@types/mark.js": "^8.11.8",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "prettier": "^2.7.1",
    "react-app-rewired": "^2.2.1",
    "react-scripts": "^5.0.1",
    "typescript": "^4.7.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tagspaces/tagspaces-extensions.git"
  },
  "bugs": {
    "url": "https://github.com/tagspaces/tagspaces-extensions/issues"
  }
}
