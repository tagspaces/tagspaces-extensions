{
  "name": "@tagspaces/md-editor",
  "version": "1.0.0",
  "description": "TagSpaces extension for editing notes in markdown format",
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:dev": "react-app-rewired build",
    "build:env": "npx env-cmd -f .env.development react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "code-format": "npx prettier --write ."
  },
  "keywords": [
    "md",
    "markdown",
    "editor",
    "tagspaces"
  ],
  "author": {
    "name": "TagSpaces UG",
    "email": "info@tagspaces.org",
    "url": "https://www.tagspaces.org"
  },
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@milkdown/core": "4.11.2",
    "@milkdown/plugin-clipboard": "4.11.2",
    "@milkdown/plugin-emoji": "4.11.2",
    "@milkdown/plugin-history": "4.11.2",
    "@milkdown/plugin-listener": "4.11.2",
    "@milkdown/plugin-math": "4.11.2",
    "@milkdown/plugin-slash": "4.11.2",
    "@milkdown/plugin-table": "4.11.2",
    "@milkdown/plugin-tooltip": "4.11.2",
    "@milkdown/preset-commonmark": "4.11.2",
    "@milkdown/preset-gfm": "4.11.2",
    "@milkdown/react": "4.11.2",
    "@milkdown/theme-nord": "4.11.2",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "material-design-icons": "^3.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remark-directive": "^2.0.0"
  },
  "devDependencies": {
    "react-app-rewired": "^2.1.8",
    "react-scripts": "4.0.3",
    "typescript": "4.4.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
