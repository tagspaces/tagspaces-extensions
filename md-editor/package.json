{
  "name": "@tagspaces/md-editor",
  "version": "1.0.6",
  "description": "TagSpaces extension for editing notes in markdown format",
  "homepage": "./",
  "main": "build/index.js",
  "files": [
    "build/"
  ],
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:dev": "react-app-rewired build",
    "build:env": "npx env-cmd -f .env.development react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "code-format": "npx prettier --write .",
    "pkg-publish": "npm publish --access public"
  },
  "keywords": [
    "md",
    "markdown",
    "editor",
    "tagspaces"
  ],
  "author": "TagSpaces UG <info@tagspaces.org> (https://www.tagspaces.org)",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "11.10.0",
    "@emotion/styled": "11.10.0",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.9.2",
    "@codemirror/basic-setup": "^0.20.0",
    "@codemirror/lang-markdown": "^0.20.1",
    "@milkdown/core": "6.3.2",
    "@milkdown/plugin-clipboard": "6.3.2",
    "@milkdown/plugin-diagram": "6.3.2",
    "@milkdown/plugin-emoji": "6.3.2",
    "@milkdown/plugin-history": "6.3.2",
    "@milkdown/plugin-listener": "6.3.2",
    "@milkdown/plugin-math": "6.3.2",
    "@milkdown/plugin-prism": "6.3.2",
    "@milkdown/plugin-slash": "6.3.2",
    "@milkdown/plugin-tooltip": "6.3.2",
    "@milkdown/preset-commonmark": "6.3.2",
    "@milkdown/preset-gfm": "6.3.2",
    "@milkdown/react": "6.3.2",
    "@milkdown/theme-nord": "6.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-loading-skeleton": "^3.1.0",
    "remark-directive": "^2.0.1"
  },
  "devDependencies": {
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "react-app-rewired": "^2.2.1",
    "react-scripts": "^5.0.1",
    "typescript": "^4.7.4",
    "prettier": "^2.7.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tagspaces/tagspaces-extensions.git"
  },
  "bugs": {
    "url": "https://github.com/tagspaces/tagspaces-extensions/issues"
  }
}
