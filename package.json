{
  "name": "@tagspaces/extensions",
  "version": "1.0.26",
  "description": "Providing support for legacy TagSpaces extensions",
  "homepage": "https://www.tagspaces.org",
  "author": "TagSpaces UG <info@tagspaces.org> (https://www.tagspaces.org)",
  "files": [
    "archive-viewer/",
    "document-viewer/",
    "ebook-viewer/",
    "html-editor/",
    "html-viewer/",
    "image-viewer/",
    "json-editor/",
    "legacy/",
    "libs/",
    "md-viewer/",
    "md-editor/build/",
    "media-player/static/",
    "media-player/index.html",
    "mhtml-viewer/",
    "pdf-viewer/",
    "rtf-viewer/",
    "spreadsheet-viewer/",
    "text-editor/",
    "text-viewer/",
    "url-viewer/"
  ],
  "scripts": {
    "updatelibs": "node scripts/update-libs.js",
    "copy-build": "shx cp -rL media-player/build/static media-player && shx cp media-player/build/index.html media-player",
    "drop-build": "npx rimraf media-player/index.html media-player/static",
    "pkg-publish": "npm run copy-build && npm publish --access public && npm run drop-build"
  },
  "devDependencies": {
    "bootstrap": "~3.4.1",
    "bootswatch": "~3.4.1",
    "dompurify": "~2.3.3",
    "font-awesome": "^4.7.0",
    "i18next": "~10.2.2",
    "jquery": "~3.6.0",
    "jquery-i18next": "~1.2.1",
    "marked": "~3.0.8",
    "mousetrap": "~1.5.3",
    "readability": "https://github.com/mozilla/readability.git#master",
    "shelljs": "^0.8.4",
    "summernote": "~0.8.20"
  },
  "license": "MIT",
  "engines": {
    "yarn": "please-use-npm",
    "npm": ">= 6.14.15"
  }
}
