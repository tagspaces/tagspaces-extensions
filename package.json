{
  "name": "@tagspaces/extensions",
  "version": "1.0.87",
  "description": "Providing support for legacy TagSpaces extensions",
  "homepage": "https://www.tagspaces.org",
  "author": "TagSpaces UG <info@tagspaces.org> (https://www.tagspaces.org)",
  "files": [
    "3d-viewer/",
    "archive-viewer/",
    "common/",
    "document-viewer/",
    "ebook-viewer/",
    "html-editor/",
    "html-viewer/",
    "image-viewer/",
    "json-editor/",
    "libs/",
    "md-viewer/",
    "md-editor/build/",
    "md-editor/public/",
    "md-editor/src/",
    "md-editor/package.json",
    "media-player/static/",
    "media-player/index.html",
    "media-player/package.json",
    "media-player/public/",
    "media-player/src/",
    "mindmap-viewer/",
    "mhtml-viewer/",
    "msg-viewer/",
    "pdf-viewer/",
    "rtf-viewer/",
    "slides-viewer/",
    "spreadsheet-viewer/",
    "text-editor/",
    "text-viewer/",
    "url-viewer/"
  ],
  "scripts": {
    "updatelibs": "node common/update-libs.js",
    "build": "node postversion.js && npm run build-media-player && npm run build-md-editor",
    "build-media-player": "cd media-player && npm install && npm run build && cd ..",
    "build-md-editor": "cd md-editor && npm install && npm run build && cd ..",
    "copy-build": "shx cp -rL media-player/build/static media-player && shx cp media-player/build/index.html media-player",
    "drop-build": "npx rimraf media-player/index.html media-player/static",
    "pkg-publish": "npm version patch && npm run build && npm run copy-build && npm publish --access public && npm run drop-build"
  },
  "devDependencies": {
    "@google/model-viewer": "^2.1.1",
    "@mozilla/readability": "0.4.2",
    "bootstrap5": "npm:bootstrap@5.2.3",
    "dompurify": "~2.4.1",
    "epubjs": "0.3.93",
    "file-saver": "^2.0.5",
    "i18next": "~10.2.2",
    "jquery": "~3.6.0",
    "jsoneditor": "9.9.2",
    "jszip": "3.10.1",
    "jszip-utils": "0.1.0",
    "mark.js": "^8.11.1",
    "marked": "4.2.5",
    "markmap-lib": "^0.14.3",
    "markmap-view": "^0.14.3",
    "psd": "3.4.0",
    "reveal.js": "^4.4.0",
    "shelljs": "^0.8.5",
    "summernote": "~0.8.20",
    "tga-js": "1.1.1",
    "viewerjs": "1.11.1"
  },
  "license": "MIT",
  "engines": {
    "yarn": "please-use-npm",
    "npm": ">= 6.14.15"
  }
}
