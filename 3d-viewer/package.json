{
  "name": "@tagspaces/3d-viewer",
  "version": "1.0.90",
  "description": "A TagSpaces extension allowing you to preview in 3D of GLB and  files.",
  "homepage": "https://www.tagspaces.org",
  "main": "main.js",
  "author": {
    "name": "TagSpaces UG",
    "email": "info@tagspaces.org",
    "url": "https://www.tagspaces.org"
  },
  "license": "MIT",
  "dependencies": {},
  "tsextension": {
    "name": "3D Viewer",
    "types": [
      "viewer"
    ],
    "fileTypes": [
      {
        "ext": "glb",
        "color": "#ac21f3"
      },
      {
        "ext": "gltf",
        "color": "#ac21f3"
      },
      {
        "ext": "stl",
        "color": "#ac21f3"
      },
      {
        "ext": "obj",
        "color": "#ac21f3"
      }
    ]
  }
}