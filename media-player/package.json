{
  "name": "@tagspaces/media-player",
  "version": "1.0.191",
  "description": "A TagSpaces extension allowing playing of audio and video files.",
  "homepage": "./",
  "files": [
    "build/"
  ],
  "author": {
    "name": "TagSpaces GmbH",
    "email": "info@tagspaces.org",
    "url": "https://www.tagspaces.org"
  },
  "license": "MIT",
  "scripts": {
    "dev": "vite dev --host",
    "build": "tsc && vite build",
    "build-dev": "tsc && vite build --mode development",
    "preview": "vite preview",
    "code-format": "npx prettier --write .",
    "npmpack": "npm run build-dev && npx shx cp -rL build/** ../../release/app/node_modules/@tagspaces/extensions/media-player && npx shx cp package.json ../../release/app/node_modules/@tagspaces/extensions/media-player"
  },
  "dependencies": {
    "@emotion/react": "11.10.0",
    "@emotion/styled": "11.10.0",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.9.2",
    "@tagspaces/tagspaces-common": "^4.5.39",
    "@vidstack/react": "^1.12.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "prettier": "^2.7.1",
    "@vitejs/plugin-react": "^4.2.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "tsextension": {
    "name": "Media Player",
    "types": [
      "viewer"
    ],
    "color": "#c5e4f9",
    "fileTypes": [
      {
        "ext": "flac"
      },
      {
        "ext": "mov"
      },
      {
        "ext": "acc"
      },
      {
        "ext": "mkv"
      },
      {
        "ext": "aac"
      },
      {
        "ext": "3g2"
      },
      {
        "ext": "3gp"
      },
      {
        "ext": "mp3"
      },
      {
        "ext": "lrv"
      },
      {
        "ext": "mp4"
      },
      {
        "ext": "m4a"
      },
      {
        "ext": "oga"
      },
      {
        "ext": "ogg"
      },
      {
        "ext": "ogv"
      },
      {
        "ext": "ogx"
      },
      {
        "ext": "opus"
      },
      {
        "ext": "spx"
      },
      {
        "ext": "wav"
      },
      {
        "ext": "wave"
      },
      {
        "ext": "webm"
      }
    ]
  }
}